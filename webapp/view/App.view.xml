<mvc:View controllerName="com.synconic.practics.SAPUI5_Walkthrough.controller.App" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" >
    <App class="sapUiDemoWT">
        <pages>
            <Page title="{i18n>homePageTitle}">
            	<headerContent>
					<Button
						icon="sap-icon://hello-world"
						press="onOpenDialog" tooltip="helloworld"/>
				</headerContent>
               <content>
                  <mvc:XMLView viewName="com.synconic.practics.SAPUI5_Walkthrough.view.HelloPanel"/>
                   <mvc:XMLView viewName="com.synconic.practics.SAPUI5_Walkthrough.view.InvoiceList"/>
               </content>
            </Page>
        </pages>
    </App>
</mvc:View>